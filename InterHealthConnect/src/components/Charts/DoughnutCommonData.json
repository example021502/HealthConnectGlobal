{
  "workLoadData": {
    "data": {
      "labels": ["Load", "Worked Hours"],
      "datasets": [
        {
          "label": "My First Dataset",
          "data": [300, 100],
          "backgroundColor": ["#365195", "#80B7E5"],
          "hoverOffset": 2,
          "borderWidth": 0,
          "borderRadius": 1,
          "spacing": 0
        }
      ]
    },

    "options": {
      "cutout": "70%",
      "plugins": {
        "legend": {
          "display": false
        },
        "datalabels": {
          "display": false
        }
      }
    }
  },

  "patientsByCondition": {
    "data": {
      "labels": ["Stable", "Fair", "Serious", "Critical"],
      "datasets": [
        {
          "data": [300, 100, 400, 300],
          "backgroundColor": ["#365195", "#80B7E5", "#16aebb", "#CB3333"],
          "hoverOffset": 2,
          "borderWidth": 0,
          "borderRadius": 1,
          "spacing": 0
        }
      ]
    },

    "options": {
      "responsive": true,
      "maintainAspectRatio": false,
      "cutout": "70%",
      "plugins": {
        "datalabels": {
          "display": false
        },
        "tooltip": {
          "enabled": true
        },
        "legend": {
          "display": false,
          "position": "right",
          "labels": {
            "usePointStyle": true,
            "pointStyle": "circle",
            "boxWidth": 8,
            "boxHeight": 8,
            "font": {
              "size": 10,
              "family": "sans-serif"
            }
          }
        }
      }
    }
  }
}
